{
	"name": "minovative-mind-vscode",
	"displayName": "Minovative Mind",
	"description": "Integrates Google Gemini into VS Code with project context awareness.",
	"version": "0.0.1",
	"publisher": "YourPublisherName",
	"repository": {
		"type": "git",
		"url": "https://github.com/Quarantiine/minovative-mind-vscode.git"
	},
	"engines": {
		"vscode": "^1.80.0"
	},
	"categories": [
		"Other",
		"Machine Learning",
		"Programming Languages",
		"AI",
		"Chat"
	],
	"activationEvents": [],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "minovative-mind",
					"title": "Minovative Mind",
					"icon": "media/minovative-logo-192x192.png"
				}
			]
		},
		"views": {
			"minovative-mind": [
				{
					"id": "minovativeMindSidebarView",
					"name": "Chat",
					"type": "webview",
					"contextualTitle": "Minovative Mind",
					"icon": "media/minovative-logo-192x192.png"
				}
			]
		},
		"submenus": [
			{
				"id": "minovativeMindSubmenu",
				"label": "Minovative Mind"
			}
		],
		"commands": [
			{
				"command": "minovative-mind.modifySelection",
				"title": "Custom Modifications",
				"category": "Minovative Mind"
			},
			{
				"command": "minovative-mind.explainSelection",
				"title": "Generate Explanation",
				"category": "Minovative Mind"
			}
		],
		"menus": {
			"editor/context": [
				{
					"submenu": "minovativeMindSubmenu",
					"title": "Minovative Mind",
					"when": "editorHasSelection",
					"group": "9_minovativemind@1"
				}
			],
			"minovativeMindSubmenu": [
				{
					"command": "minovative-mind.modifySelection",
					"group": "1_modification@1"
				},
				{
					"command": "minovative-mind.explainSelection",
					"group": "2_analysis@1"
				}
			]
		},
		"keybindings": [
			{
				"command": "minovative-mind.modifySelection",
				"key": "ctrl+m",
				"mac": "cmd+m",
				"when": "editorHasSelection && editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/bluebird": "^3.5.42",
		"@types/glob": "^8.1.0",
		"@types/mocha": "^10.0.6",
		"@types/vscode": "^1.80.0",
		"@typescript-eslint/eslint-plugin": "^7.7.1",
		"@typescript-eslint/parser": "^7.7.1",
		"@vscode/codicons": "^0.0.36",
		"@vscode/test-electron": "^2.3.9",
		"eslint": "^8.57.0",
		"glob": "^10.3.10",
		"mocha": "^10.4.0",
		"ts-loader": "^9.5.1",
		"typescript": "^5.4.5",
		"webpack": "^5.91.0",
		"webpack-cli": "^5.1.4"
	},
	"dependencies": {
		"@google/generative-ai": "^0.24.1",
		"bluebird": "^3.7.2",
		"ignore": "^7.0.4"
	}
}
